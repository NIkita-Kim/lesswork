#!/usr/bin/env node

/**
 * Lesswork Command Line Interface.
 * 
 * Provides the local interface for the CLI.
 * 
 */

require('lesswork-framework/lib/env');

const Kernel = require('lesswork-framework/src/Kernel');
const appConfig = require('./config/app');

new Kernel().handle(function () {
  require('lesswork-framework/src/Work')(appConfig);
}, appConfig.providers.concat(appConfig.workProviders));